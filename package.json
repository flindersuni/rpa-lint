{
  "name": "rpa-lint",
  "version": "1.1.0",
  "description": "A CLI app to lint UiPath projects against rules developed by the Flinders RPA team",
  "homepage": "https://github.com/flindersuni/rpa-lint/#readme",
  "bugs": {
    "url": "https://github.com/flindersuni/rpa-lint/issues",
    "email": "corey.wallis@flinders.edu.au"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flindersuni/rpa-lint.git"
  },
  "private": true,
  "keywords": [
    "rpa",
    "robotic-process-automation",
    "uipath",
    "university",
    "academic"
  ],
  "scripts": {
    "test": "mocha --require esm",
    "lint": "eslint .",
    "docs": "jsdoc -c .jsdocrc.json",
    "coverage": "c8 mocha --require esm"
  },
  "bin": {
    "rpa-lint": "bin/rpa-lint"
  },
  "author": "Corey Wallis <corey.wallis@flinders.edu.au>",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "c8": "^5.0.4",
    "eslint": "^6.5.0",
    "eslint-config-jquery": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^15.9.5",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-security": "^1.4.0",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.1",
    "sinon": "^7.5.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.20.1",
    "compare-versions": "^3.5.1",
    "esm": "^3.2.25",
    "pretty-ms": "^5.0.0",
    "sync-request": "^6.1.0",
    "xmldom": "^0.1.27",
    "xpath": "0.0.27"
  },
  "directories": {
    "test": "test",
    "doc": "doc"
  },
  "esm": {
    "cjs": true
  },
  "mocha": {
    "require": [
      "esm"
    ],
    "recursive": true
  }
}
